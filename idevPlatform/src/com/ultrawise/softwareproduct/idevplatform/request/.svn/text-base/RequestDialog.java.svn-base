package com.ultrawise.softwareproduct.idevplatform.request;

import android.app.Dialog;

/**
 * Created by JNinBer on 2014/5/5 0005.
 */
public class RequestDialog {
     public  boolean  mShowDialog;
     public  boolean  mTouchCancel;
     public  boolean  mBackCancel;
     public  boolean  mSystemCancel;

    public int getBarColor() {
        return barColor;
    }

    public void setBarColor(int barColor) {
        this.barColor = barColor;
    }

    public  int barColor;
    public RequestDialog(boolean isShowDialog ,boolean isTouchCancel,boolean isSystemCancel,boolean isBackCancel){
        this.mShowDialog=isShowDialog;
        this.mSystemCancel=isSystemCancel;
        this.mBackCancel=isBackCancel;
        this.mTouchCancel=isTouchCancel;
    }
     public  RequestDialog (boolean isShowDialog  ,boolean isSystemCancel){
         this.mShowDialog=isShowDialog;
         this.mSystemCancel=isSystemCancel;
     }
    public boolean isShowDialog() {
        return mShowDialog;
    }
    public void setShowDialog(boolean isShowDialog) {
        this.mShowDialog = isShowDialog;
    }
    public boolean isTouchCancel() {
        return mTouchCancel;
    }

    public void setTouchCancel(boolean isTouchCancel) {
        this.mTouchCancel = isTouchCancel;
    }

    public boolean isBackCancel() {
        return mBackCancel;
    }
    public void setBackCancel(boolean isBackCancel) {
        this.mBackCancel = isBackCancel;
    }
    public boolean isSystemCancel() {
        return mSystemCancel;
    }
    public void setSystemCancel(boolean isSystemCancel) {
        this.mSystemCancel = isSystemCancel;
    }
}
